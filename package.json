{
  "name": "uilicious-docs",
  "version": "1.0.0",
  "description": "Guide and documentation on how to use UI-licious",
  "scripts": {
    "gitbook-help": "gitbook help",
    "install": "gitbook --version; gitbook install",
    "build": "gitbook --version; gitbook build",
    "serve": "gitbook --version; gitbook serve",
    "pdf": "gitbook --version; gitbook pdf . _out/uilicious-docs.pdf",
    "epub": "gitbook --version; gitbook epub . _out/uilicious-docs.epub",
    "deploy": "gitbook --version; gitbook build && netlify deploy --dir=_book",
    "deploy--production": "gitbook --version; gitbook build && netlify deploy --prod --dir=_book"
  },
  "author": "UILICIOUS PRIVATE LIMITED",
  "license": "UNLICENSED",
  "dependencies": {
    "gitbook-cli": "=2.3.2",
    "gitbook-plugin-addcssjs": "=1.0.2",
    "gitbook-plugin-custom-favicon": "=0.0.4",
    "gitbook-plugin-docsearch": "=1.2.0",
    "gitbook-plugin-prism": "=2.4.0",
    "gitbook-plugin-toggle-chapters": "=0.0.3",
    "gitbook-plugin-youtube": "=1.1.0",
    "prism-themes": "=1.0.1"
  },
  "devDependencies": {
    "netlify": "^4.5.1",
    "netlify-cli": "^2.63.2"
  }
}
